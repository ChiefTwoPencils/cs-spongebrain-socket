ackage cs;

import java.util.ArrayList;
import java.util.List;

public class GenMergeSort {
	
	public static <T extends Comparable<T>> T[] mergeSort(T[] arr) {
		if (arr.length < 2) {
			return arr;
		}
		return arr;
	}
	
	public static <T extends Comparable<T>> T[] merge(T[] first, T[] second) {
		List<T> merged = new ArrayList<>();
		
		return (T[]) merged.toArray();
	}
}
